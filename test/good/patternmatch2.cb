// declare an algebraic data type
type Foo {
    Bar,
    Foo,
    Baz(int, Foo),
}

def main() {
    let var = Baz(420, Bar);
    // pattern match
    let x = match var {
        Baz(n,m) => {
            printInt(n);
            match m {
                Bar => printString("got bar")
            }
        }
    };
}
