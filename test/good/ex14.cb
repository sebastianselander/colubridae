def foo(x: int) -> fn(int, string, bool, int) -> () {
    return \a b c d -> {
        printInt(a);
        printString(b);
        printBool(c);
        printInt(d+x);
    }
}

def main() {
    let f = foo(370);
    f(10,"yoo",true,50);
}


def printBool(b: bool) {
    if b {
        printString("true")
    } else {
        printString("false")
    }
}
